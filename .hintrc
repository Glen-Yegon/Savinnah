{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]",
          "meta[name=theme-color]",
          "img[fetchpriority]",
          "iframe[loading]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color",
          "mask-image",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}